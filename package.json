{
  "name": "longevity-chatbot",
  "version": "1.0.0",
  "description": "Medical chatbot with RAG pipeline using FastAPI and React",
  "main": "index.js",
  "scripts": {
    "build": "cd client && npm install && npm run build",
    "start": "cd server && python -m uvicorn main:app --host 0.0.0.0 --port $PORT",
    "dev:server": "cd server && uvicorn main:app --reload",
    "dev:client": "cd client && npm start",
    "install:client": "cd client && npm install",
    "postinstall": "npm run install:client"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "keywords": [
    "medical",
    "chatbot",
    "rag",
    "fastapi",
    "react",
    "openai",
    "pubmed"
  ]
}
